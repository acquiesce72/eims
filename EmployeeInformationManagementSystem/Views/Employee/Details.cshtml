                    </tr>
                </thead>
                <tbody>
                    @foreach (var eb in ViewBag.EmployeeBeneficiaries)
                    {
                        DateTime noww = DateTime.Today;
                        int agee = noww.Year - eb.birth_date.Year;
                        if (noww < eb.birth_date.AddYears(agee))
                        {
                            agee--;
                        }

                        DateTime dtt = Convert.ToDateTime(eb.birth_date.ToString());
                        var datee = dtt.ToShortDateString();

                        <tr>
                            @if (Session["User_role"].ToString() == "Administrator")
                            {
                                <td>
                                    <button type="button" class="btn btn-xs btn-warning btn-primary-outline" onclick="location.href='@Url.Action("EditBeneficiaries", "Employee" , new { id=eb.r_id, back = eb.employee_id })'"><span class="fa fa-edit"></span></button>
                                    <button type="button" class="btn btn-xs btn-danger btn-primary-outline" onclick="location.href='@Url.Action("DeleteBeneficiaries", "Employee" , new { id=eb.r_id, back = eb.employee_id })'"><span class="fa fa-remove"></span></button>
                                </td>
                            }
                            
                            <td>
                                @eb.firstname
                            </td>
                            <td>
                                @eb.middlename
                            </td>
                            <td>
                                @eb.lastname
                            </td>
                            <td>
                                @datee
                            </td>
                            <td>
                                @agee
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->

