@model EmployeeInformationManagementSystem.Models.Employee

@{
    ViewBag.Title = "Employee Details";
    var url = Url.RequestContext.RouteData.Values["id"];
}

<section class="content-header">
    <h1>
        @if (Session["User_role"].ToString() != "Employee")
        {
            <small><b>Details of @Html.DisplayFor(model => model.firstname) @Html.DisplayFor(model => model.middlename) @Html.DisplayFor(model => model.lastname)</b></small>
        }
        &nbsp;
    </h1>

    @{
        if (Session["User_role"].ToString() == "Employee")
        {
            <ol class="breadcrumb">
                <li class="active"><i class="fa fa-file-text"></i> My Employee Record</li>
            </ol>
        }
        else
        {
            <ol class="breadcrumb">
                <li><i class="fa fa-users"></i> Employee</li>
                <li><i class="fa fa-list-alt"></i> List of Employees</li>
                <li class="active"><i class="fa fa-info-circle"></i> Employee Details</li>
            </ol>
        }
    }
    
</section>

<section class="invoice">
    <div class="row">
        <div class="col-xs-12">
            <h2 class="page-header">
                <i class="fa fa-user"></i> Employee Details
                @if (Session["User_role"].ToString() == "Administrator")
                {
                    <small class="pull-right">Record ID: @Html.DisplayFor(model => model.employee_id)</small>
                }           
            </h2>
        </div>
    </div>
    <div class="row invoice-info">
        <div class="col-sm-4 invoice-col">
            <address>
                <strong>Employee Number</strong><br />
                @Html.TextBoxFor(model => model.employee_number, new { @class = "form-control no-gray", @readonly = "readonly" })<br />
                <strong>First Name</strong><br />
                @Html.TextBoxFor(model => model.firstname, new { @class = "form-control no-gray", @readonly = "readonly" })<br />
                <strong>Middle Name</strong><br />
                @Html.TextBoxFor(model => model.middlename, new { @class = "form-control no-gray", @readonly = "readonly" })<br />
                <strong>Last Name</strong><br />
                @Html.TextBoxFor(model => model.lastname, new { @class = "form-control no-gray", @readonly = "readonly" })<br />
                <strong>Birth Date</strong><br />
                @{
                    DateTime dt = Convert.ToDateTime(Model.birth_date.ToString());
                    var date = dt.ToShortDateString();
                }
                <input class="form-control no-gray" type="text" value="@date" readonly /><br />
                <strong>Birth Place</strong><br />
                @Html.TextBoxFor(model => model.birth_place, new { @class = "form-control no-gray", @readonly = "readonly" })<br />
               
                @{ 
                    DateTime now = DateTime.Today;
                    int age = now.Year - Model.birth_date.Year;
                    if (now < Model.birth_date.AddYears(age))
                    {
                        age--;
                    }
                }

                 <strong>Age</strong><br />
                <input class="form-control no-gray" type="text" value="@age" readonly /><br />
                <strong>Gender</strong><br />
                @Html.TextBoxFor(model => model.gender, new { @class = "form-control no-gray", @readonly = "readonly" })<br />
                <strong>Civil Status</strong><br />
                @Html.TextBoxFor(model => model.civil_status, new { @class = "form-control no-gray", @readonly = "readonly" })<br />
                <strong>Contact Number</strong><br />
                @Html.TextBoxFor(model => model.contact_number, new { @class = "form-control no-gray", @readonly = "readonly" })<br />
                
                
            </address>
        </div>

        <div class="col-sm-4 invoice-col">
            <address>
                
                <strong>Address</strong><br />
                @Html.TextBoxFor(model => model.address, new { @class = "form-control no-gray", @readonly = "readonly" })<br />
                <strong>Height</strong><br />
                @Html.TextBoxFor(model => model.height, new { @class = "form-control no-gray", @readonly = "readonly" })<br />
                <strong>Weight</strong><br />
                @Html.TextBoxFor(model => model.weight, new { @class = "form-control no-gray", @readonly = "readonly" })<br />
                <strong>Citizenship</strong><br />
                @Html.TextBoxFor(model => model.citizenship, new { @class = "form-control no-gray", @readonly = "readonly" })<br />
                <strong>Blood Type</strong><br />
                @Html.TextBoxFor(model => model.blood_type, new { @class = "form-control no-gray", @readonly = "readonly" })<br />
                <strong>Contact Person</strong><br />
                @Html.TextBoxFor(model => model.contact_person, new { @class = "form-control no-gray", @readonly = "readonly" })<br />
                <strong>Marriage Date</strong><br />
                @{
                    var mad = "";

                    if(Model.marriage_date != null)
                    {
                        DateTime md = Convert.ToDateTime(Model.marriage_date.ToString());
                        mad = md.ToShortDateString();
                    }

                }
                <input class="form-control no-gray" type="text" value="@mad" readonly /><br />
                <strong>Marriage Place</strong><br />
                @Html.TextBoxFor(model => model.marriage_place, new { @class = "form-control no-gray", @readonly = "readonly" })<br />
                <strong>Spouse Name</strong><br />
                @Html.TextBoxFor(model => model.spouse_name, new { @class = "form-control no-gray", @readonly = "readonly" })<br />
                <strong>Spouse Occupation</strong><br />
                @Html.TextBoxFor(model => model.spouse_occupation, new { @class = "form-control no-gray", @readonly = "readonly" })<br />
                
                
            </address>
        </div>

        <div class="col-sm-4 invoice-col">

            &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
            &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
            &nbsp;&nbsp;&nbsp;&nbsp;

            <img src="@Url.Action("EmployeePicture", "Home", new { id = url })" class="img-circle pull-center" alt="User Image" width="190" height="190">
            <center><strong>Employee Image</strong></center>
            <br />

            <strong>Fathers Name</strong><br />
            @Html.TextBoxFor(model => model.father_name, new { @class = "form-control no-gray", @readonly = "readonly" })<br />
            <strong>Fathers Occupation</strong><br />
            @Html.TextBoxFor(model => model.father_occupation, new { @class = "form-control no-gray", @readonly = "readonly" })<br />
            <strong>Mothers Name</strong><br />
            @Html.TextBoxFor(model => model.mother_name, new { @class = "form-control no-gray", @readonly = "readonly" })<br />
            <strong>Mothers Occupation</strong><br />
            @Html.TextBoxFor(model => model.mother_occupation, new { @class = "form-control no-gray", @readonly = "readonly" })<br />
            <strong>Confirmation Date</strong><br />
            @{
                DateTime cd = Convert.ToDateTime(Model.confirmation_date.ToString());
                var cod = cd.ToShortDateString();
            }
            <input class="form-control no-gray" type="text" value="@cod" readonly /><br />
            <strong>Confirmation Place</strong><br />
            @Html.TextBoxFor(model => model.confirmation_place, new { @class = "form-control no-gray", @readonly = "readonly" })<br />

            @{
                var status = "";

                if (Model.is_active)
                {
                    status = "Active";
                }

                else
                {
                    status = "Passive";
                }
            }

            <strong>Employment Status</strong><br />
            <input class="form-control no-gray" type="text" value="@status" readonly /><br />

        </div>
    </div>

    <div class="row">
        <div class="col-xs-12">
